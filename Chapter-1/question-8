fn main() {
    let a = [1, 2, 3, 4, 5, 6, 7, 8, 9];

    let b = &a[1..3];
    println!("a. Slice of 2nd and 3rd element: {:?}", b);

    let c = &a[..7];
    println!("c. Omit the end index of the slice: {:?}", c);

    let d = &a[..];
    println!("d. Omit both start and end index of the slice: {:?}", d);
}
